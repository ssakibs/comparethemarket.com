{
  "extends": [
    "eslint:recommended",
    "airbnb/base"
  ],
  "rules": {
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-useless-escape": 0,
    "arrow-body-style": 0,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "globals": {}
}